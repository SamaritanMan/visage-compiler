		    					$typeReference.rtype,
		    					$mods,
		    					bValue,
		    					bStatus,
		    					oValue
		    				);
	    	}
	    	// Documentation comment (if any)
	    	//
			setDocComment($value, docComment);
			
			// Tree span
			//
			endPos($value); 
