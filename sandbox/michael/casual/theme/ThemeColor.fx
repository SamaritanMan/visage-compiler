package casual.theme;

import javafx.ui.Color;

public class ThemeColor extends Color
{
}

WHITE:ThemeColor = ThemeColor
{
    red:        1.0
    green:      1.0
    blue:       1.0
    opacity:    1.0
};

BLACK:ThemeColor = ThemeColor
{
    red:        0.0
    green:      0.0
    blue:       0.0
    opacity:    1.0
};
