/**
 * regression test:  test that the default value of a sequence is the empty sequence.
 *  Test both attributes and local vars.  And both "primitives" and Objects (String).
 *
 * @test
 * @run
 */

import java.lang.System;

class defaultseq {
   var achorti : Number[];
}

var bulorpt : defaultseq[];
var rebinz : String[];

System.out.println(bulorpt);
System.out.println(rebinz);

insert new defaultseq into bulorpt;
insert 3.14159265358979 into bulorpt[0].achorti;
insert "foolish" into rebinz;

System.out.println(bulorpt[0].achorti);
System.out.println(rebinz);

