/*
 * JFXC-3243:  Using mixin class causes IllegalArgumentException arising from visage.reflect.VisageLocal$VarMember.getValue.
 *
 * @compilefirst jfxc3243A.visage
 * @compilefirst jfxc3243B.visage
 * @test
 * @run
 *
 */

import visage.reflect.*;

var b = jfxc3243B{};
var context = VisageLocal.getContext();
var mirror = context.mirrorOf(b); 
var cls = context.findClass("jfxc3243B"); 
var fld = cls.getVariable("a");
var a = fld.getValue(mirror); 
println("{a!=null}");
