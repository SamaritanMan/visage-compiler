 *
 * mixin A should be initialized just once
 */
mixin class A           {init{visage.lang.FX.println("init A");}}
mixin class E extends A {init{visage.lang.FX.println("init E");}}
class B extends A       {init{visage.lang.FX.println("init B");}}
class C extends A, B, E {init{visage.lang.FX.println("init C");}}

C{};
