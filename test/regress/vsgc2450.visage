/*****  Test generator ****

var by : Byte = 1;
var sh : Short = 10;
var ii : Integer = 100;
var lo : Long = 2147383648;
var fl : Float = 1234.5;
var db : Double = 6.12e40;

class P {
  var n: String;
  var r: Number;
}

def vs = [
	P {n: 'by' r: by},
	P {n: 'sh' r: sh},
	P {n: 'ii' r: ii},
	P {n: 'lo' r: lo},
	P {n: 'fl' r: fl},
	P {n: 'db' r: db}
];

class O {
  var n: String;
  var m: String;
  var f: function(:Number,:Number):Number;
}

def os = [
	O {n: '+' m: 'plus'  f: function(x,y) {x+y}}
	O {n: '-' m: 'minus' f: function(x,y) {x-y}}
	O {n: '*' m: 'mult'  f: function(x,y) {x*y}}
	O {n: '/' m: 'div'   f: function(x,y) {x/y}}
	O {n: 'mod' m: 'mod' f: function(x,y) {x mod y}}
];

for (v1 in vs, v2 in vs, o in os) {
  println('def {v1.n}_{v2.n}_{o.m} = bind {v1.n} {o.n} {v2.n};');
}

for (v1 in vs, v2 in vs, o in os) {
  println('println( {v1.n}_{v2.n}_{o.m} );');
}

for (v1 in vs) {
  println('{v1.n} += {v1.n};');
}

for (v1 in vs, v2 in vs, o in os) {
  println('println( {v1.n}_{v2.n}_{o.m} );');
}

println('---------------------------------------');

for (v1 in vs, v2 in vs, o in os) {
  println('println( {v1.n} {o.n} {v2.n} );');
}

for (v1 in vs, v2 in vs, o in os) {
  println('println( ({v1.n} * 2) {o.n} ({v2.n} * 2) );');
}

****/
