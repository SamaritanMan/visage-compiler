/* Binding Overhaul test: literals
 *
 * @test
 * @run
 */

function run() {
   var vi = 5;
   var vd = 5.5;
   var vb = true;
   var vs = "there";

   var i = bind 22;
   var d : Double = bind 3.14159265368979;
   var b = bind false;
   var s = bind "hello";

   var qi = bind [22, vi];
   var qd : Double[] = bind [3.14159265368979, vd];
   var qb = bind [false, vb];
   var qs = bind ["hello", vs];
   var qn : String[] = null;
   var qe : java.lang.Object[] = [];

   println(i);
   println(d);
   println(b);
   println(s);
   println(qi);
   println(qd);
   println(qb);
   println(qs);
   println(qn);
   println(qe);
}
