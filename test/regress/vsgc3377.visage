/**
 * VSGC-3377 : regression: too strict generic code generated by bound translation.
 *
 * @test
 * @author Maurizio
 */

class A {} 
class B extends A {} 

class C { 
   var a:A[]; 
} 

var seq_b: B[]; 

C{ 
   a: bind if (true) seq_b else seq_b; //removing the bind makes the problem disappear! 
} 

