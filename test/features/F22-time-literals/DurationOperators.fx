/**
 * Exercize various operators on Duration.
 *
 * @test
 * @run
 */

import java.lang.System;

// duration addition
var r = 3ms + 2ms;
System.out.println("3ms + 2ms = {r}");

// duration subtraction
r = 5ms - 2ms;
System.out.println("5ms - 2ms = {r}");

// multiply by integer
r = 3ms * 2;
System.out.println("3ms * 2 = {r}");
// multiply by integer
r = 2 * 3ms;
System.out.println("2 * 3ms = {r}");

// multiply by number
r = 2s * 2.5;
System.out.println("2s * 2.5 = {r}");
// multiply by number
r = 2.5 * 2s;
System.out.println("2.5 * 2s = {r}");

// divide by integer
r = 3ms / 2;
System.out.println("3ms / 2 = {r}");

// divide by number
r = 2.5s / 2.5;
System.out.println("2.5s / 2.5 = {r}");

// divide by another Duration
var ratio = 2.5ms/5.0ms;
System.out.println("2.5ms / 5.0ms = {ratio}");



